{
  "presets": [
    ["env", {
      "targets": {
        "browsers": "> 1%",
        "uglify": true
      },
      "useBuiltIns": true,
      "modules": false
    }],
    "stage-0",
    "react"
  ],
  "plugins": [
    ["transform-runtime", {
      "polyfill": false,
      "regenerator": true
    }],
    "transform-async-to-generator",
    "transform-decorators-legacy",
    "syntax-dynamic-import",
    "transform-object-rest-spread",
    "transform-class-properties",
    "universal-import"
  ]
}
