{
  "name": "react-storefront",
  "version": "4.0.0",
  "description": "Build and deploy e-commerce progressive web apps in record time.",
  "repository": "https://github.com/moovweb/react-storefront",
  "license": "UNLICENSED",
  "private": true,
  "devEngines": {
    "node": "8.x || 9.x || 10.x"
  },
  "devDependencies": {
    "@material-ui/core": "^3.0.0",
    "@material-ui/icons": "^3.0.0",
    "gh-pages": "^1.2.0",
    "glob": "^7.1.3",
    "husky": "^1.1.2",
    "lerna": "^3.10.7",
    "mobx": "^4.1.0",
    "mobx-react": "^5.0.0",
    "mobx-state-tree": "^3.10.0",
    "prop-types": "^15.6.1",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-helmet": "^5.2.0",
    "react-jss": "^8.3.5",
    "react-router-dom": "^4.2.2",
    "react-transition-group": "^2.5.0",
    "react-universal-component": "^2.9.0",
    "webpack": "^4.25.1"
  },
  "scripts": {
    "build": "cd packages/react-storefront; yarn build; cd ../../; cd packages/react-storefront-moov-xdn; yarn build; cd ../../;",
    "precommit": "yarn add-license && yarn docs",
    "publish": "lerna version --force-publish=*; yarn build; yarn publish:rsf; yarn publish:moov-xdn; yarn publish:middleware; yarn publish:babel-plugin", 
    "publish:rsf": "cd packages/react-storefront/dist; yarn publish --non-interactive",
    "publish:babel-plugin": "cd packages/babel-plugin-react-storefront; yarn publish --non-interactive",
    "publish:moov-xdn": "cd packages/react-storefront-moov-xdn/dist; yarn publish --non-interactive",
    "publish:middleware": "cd packages/react-storefront-middleware; yarn publish --non-interactive",
    "docs": "cd packages/react-storefront; yarn docs; cd ../../;",
    "deploy-storybook": "gh-pages -d packages/react-storefront/storybook-static",
    "test": "cd packages/react-storefront; yarn test:unit; cd ../../;",
    "watch": "cd packages/react-storefront; yarn watch; cd ../../;",
    "add-license": "node ./tasks/addLicense",
    "link:all": "yarn build; cd packages/react-storefront/lib; npm link; yarn link; cd ../../../; cd packages/babel-plugin-react-storefront; npm link; yarn link; cd ../../; cd packages/react-storefront-moov-xdn/dist; npm link; yarn link; cd ../../../;"
  },
  "workspaces": {
    "packages": [
      "packages/*"
    ],
    "nohoist": [
      "*"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn precommit"
    }
  },
  "dependencies": {
    "lodash": "^4.17.5"
  }
}
